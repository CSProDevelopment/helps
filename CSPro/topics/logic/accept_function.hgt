<p style="margin-left: 12.00pt; margin-top: 0.00pt; margin-bottom: 0.00pt;
	 text-align: left;">&#160;</p>
<p style="margin-top: 0.00pt; margin-bottom: 0.00pt; text-align: left;"><font
	 style="font-weight: bold;"><b>Example 2:</b></font></p>
<p style="margin-top: 0.00pt; margin-bottom: 0.00pt; font-family: 'Courier New', monospace;
	 margin-left: 12px; font-weight: normal;"><font color="#000000"><font
	 color="#0000FF">PROC</font> <font color="#0000FF">GLOBAL</font><br />
&#160;<br />
<font color="#0000FF">array</font> <font color="#0000FF">string</font>
 acceptOptions(<font color="#FF0000">20</font>);<br />
&#160;<br />
<font color="#0000FF">PROC</font> ACTION<br />
<br />
<font color="#0000FF">preproc</font><br />
<br />
&#160;&#160;&#160;&#160;<font color="#0000FF">numeric</font> ctr;<br />
&#160;<br />
&#160;&#160;&#160;&#160;<font color="#0000FF">do</font> ctr = <font color="#FF0000">1</font>
 <font color="#0000FF">while</font> ctr &lt;= <font color="#0000FF">count</font>(NAME)<br />
&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;acceptOptions(ctr) = NAME(ctr);<br />
&#160;&#160;&#160;&#160;<font color="#0000FF">enddo</font>;<br />
&#160;<br />
&#160;&#160;&#160;&#160;acceptOptions(ctr) = <font color="#FF00FF">&quot;&quot;</font>;
 <font color="#008000">//&#160;mark the end of the array<br />
</font><br />
&#160;&#160;&#160;&#160;ACTION = <font color="#0000FF">accept</font>(<font
	 color="#FF00FF">&quot;Select a Person to Interview&quot;</font>,acceptOptions);</font></p>
